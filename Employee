CREATE TABLE EMPLOYEE (
EMPSEQ NUMBER PRIMARY KEY,
EMPID VARCHAR2(10),
EMPLOYEENAME VARCHAR2(20),
AGE NUMBER,
CITY VARCHAR2(15),
SALARY NUMBER,
VARIABLEPAY NUMBER,
EMPLOYEEROLE VARCHAR(20)
);

--Inserting data into Table

--Updating table Employee
UPDATE EMPLOYEE SET EMPLOYEENAME='VIKAS SINGH' WHERE EMPSEQ=2;
UPDATE EMPLOYEE SET EMPLOYEENAME='VISHAL MHATRE' WHERE EMPSEQ=3;
UPDATE EMPLOYEE SET EMPLOYEENAME='ROHAN MHATRE' WHERE EMPSEQ=4;
UPDATE EMPLOYEE SET EMPLOYEENAME='Rahul Singh' WHERE EMPSEQ=5;
